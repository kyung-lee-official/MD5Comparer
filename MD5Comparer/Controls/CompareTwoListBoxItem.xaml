<UserControl x:Class="MD5Comparer.Controls.CompareTwoListBoxItem"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:local="clr-namespace:MD5Comparer.Controls" mc:Ignorable="d" d:DesignHeight="450" d:DesignWidth="800">
	<Grid>
		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="1*"/>
			<ColumnDefinition Width="1*"/>
		</Grid.ColumnDefinitions>
		<Label x:Name="firstLabel" Grid.Column="0" Height="45" BorderThickness="1" BorderBrush="#636e72">
			<TextBlock x:Name="firstTextBlock" TextTrimming="CharacterEllipsis"></TextBlock>
		</Label>
		<Label x:Name="secondLabel" Grid.Column="1" Height="45" BorderThickness="1" BorderBrush="#636e72">
			<TextBlock x:Name="secondTextBlock" TextTrimming="CharacterEllipsis"></TextBlock>
		</Label>
	</Grid>
</UserControl>
